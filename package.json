{
	"name": "grato-shop-client",
	"version": "1.0.0",
	"description": "an e-commernce project aimed for selling cell-phones",
	"author": "Liam",
	"private": true,
	"scripts": {
		"dev:local": "nuxt",
		"dev:mobile": "nuxt --hostname 0.0.0.0 --port 8000",
		"dev": "concurrently -k -n \"dev:local,dev:mobile\" -c \"black.bgYellow,white.bgBlack\" \"yarn dev:local\" \"yarn dev:mobile\"",
		"dev-debug": "node -inspect node_modules/.bin/nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"test": "jest",
		"lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
	},
	"dependencies": {
		"nuxt": "^2.0.0"
	},
	"devDependencies": {
		"@nuxtjs/auth": "^4.8.4",
		"@nuxtjs/axios": "^5.3.6",
		"@nuxtjs/device": "^1.2.3",
		"@nuxtjs/dotenv": "^1.4.1",
		"@nuxtjs/eslint-config": "^1.0.1",
		"@nuxtjs/eslint-module": "^1.0.0",
		"@nuxtjs/google-analytics": "^2.2.1",
		"@nuxtjs/pwa": "^3.0.0-beta.19",
		"@nuxtjs/redirect-module": "^0.3.1",
		"@nuxtjs/sentry": "^3.0.1",
		"@nuxtjs/tailwindcss": "^1.0.0",
		"@vue/test-utils": "^1.0.0-beta.27",
		"babel-eslint": "^10.0.3",
		"babel-jest": "^24.1.0",
		"body-scroll-lock": "^2.6.4",
		"eslint": "^6.6.0",
		"eslint-config-prettier": "^6.5.0",
		"eslint-plugin-nuxt": ">=0.4.2",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-vue": "6",
		"jest": "^24.1.0",
		"prettier": "^1.18.2",
		"swipejs": "^2.2.14",
		"vue-currency-filter": "^3.4.3",
		"vue-fragment": "^1.5.1",
		"vue-jest": "^4.0.0-0",
		"vue-product-carousel": "^1.2.0",
		"vue-svg-loader": "^0.14.0",
		"vue-template-compiler": "^2.6.10",
		"vuelidate": "^0.7.4"
	}
}
